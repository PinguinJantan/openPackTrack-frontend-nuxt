{
  "name": "pack-track-front-end-nuxt",
  "version": "1.0.0",
  "description": "Our striking Nuxt.js project",
  "author": "Diky Arga",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "env-cmd ./.env nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "standard --fix && eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "precommit": "lint-staged",
    "cy:verify": "cypress verify",
    "cy:run": "cypress run"
  },
  "lint-staged": {
    "**/*.vue|**/*.js": [
      "prettier --print-width 100 --single-quote --trailing-comma all --write",
      "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "ignore": [
      "cypress"
    ]
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.5.1",
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/toast": "^3.0.1",
    "dayjs": "^1.7.7",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.5",
    "nuxt": "^2.6.3",
    "vee-validate": "^2.0.6",
    "vue-moment": "^4.0.0",
    "vue-multiselect": "^2.1.0",
    "vuetify": "^1.15.14"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "cross-env": "^5.1.4",
    "cypress": "^3.2.0",
    "env-cmd": "^7.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-vue": "^4.4.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "nodemon": "^1.17.2",
    "prettier-eslint": "^8.8.1",
    "standard": "^12.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
